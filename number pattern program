import java.util.Scanner;
class Main{
    public static void main (String[] args) {
        Scanner sc= new Scanner(System.in);
        System.out.print("Entr the number :");
        int num=sc.nextInt();
        
        for(int i=1;i<=num;i++){
            if(num%2==0){
                int result=(i*i)-2;
                System.out.println(result);
            }
            else{
                int result=(i*i)-1;
                System.out.println(result);
            }
        }
    }
}
