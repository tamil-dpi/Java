import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int[n];
		for(int i=0; i<n; i++) {
			arr[i]=sc.nextInt();
		}
		Arrays.sort(arr);
		int miss=arr[0];
		for(int i=0; i<n; i++) {
			if (arr[i] <= 0) continue;
			else if (arr[i]==miss) miss++;
			else if (arr[i]>miss) break;
		}
		System.out.println(miss);
	}
}

