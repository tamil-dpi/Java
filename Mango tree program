import java.util.Scanner;
class Main{
    public static void main(String [] args){
    Scanner sc= new Scanner (System.in);
    int ip1= sc.nextInt();
    int ip2= sc.nextInt();
    int m= sc.nextInt();
    
    if(m>ip1*ip2){
        System.out.print("Invalid");
    }
    else{
        if(m<=ip1 || m%ip1==1|| m%ip1==0){
            System.out.print("Mango tree");
        }
        else{
            System.out.print("Not a Mango Tree");
        }
    }
    }
}
