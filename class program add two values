class ksr{
    
    public  static void display(){
        int a=10;
        int b=30;
        int c=a+b;
        System.out.println("Sum=" + c);
    }
        public static void sakthui(int x,int y){
            int result = x*y;
            System.out.println("Mutiply="+result);
            
        }
    
}
public class Main{
	public static void main(String[]args) {
	    ksr.display();
	    //System.out.println(" this the main method");
	    ksr.sakthui(5,7);
	    
	}
}
