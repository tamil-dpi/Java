class Calculator{
    int sum(int a, int b){
        return a+b;
    }
    int sum (int a, int b,int c){
        return a+b+c;
    }
    double sum(double a,double b){
        return a+b;
    }
}
public class Main{
    public static void main(String [] args){
        Calculator c= new Calculator();
         System.out.println(c.sum(2.3,4.6));
         System.out.println(c.sum(23,4));
         System.out.println(c.sum(23,4,6));
    }
}
