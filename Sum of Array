import java.util.Scanner;
class Main {
	public static int Sum(int[] arr, int ind, int total ) {
		if(ind==-1) return total; //Base case:
		return Sum(arr, ind-1, total+arr[ind]); //Recursive case
	}
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n = sc.nextInt();
		int[] arr = new int[n];
		for (int i=0; i<n; i++) {
			arr[i]= sc.nextInt();
		}
		System.out.println(Sum(arr,n-1,0));
	}
}
