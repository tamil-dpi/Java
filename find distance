import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int x = sc.nextInt();
        int y = sc.nextInt();

        double dx = Math.pow(3 - x, 2);
        double dy = Math.pow(4 - y, 2);

        double distance = Math.sqrt(dx + dy);

        System.out.print(distance);
    }
}
